<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://tulok.vzma.hu/tags</namespace>
	<tag>
		<tag-name>textField</tag-name>
		<source>tags/textField.xhtml</source>
		<attribute>
			<name>value</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>mask</name>
		</attribute>
		<attribute>
			<name>placeholder</name>
		</attribute>
		<attribute>
			<name>tooltip</name>
		</attribute>
		<attribute>
			<name>readonly</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>textArea</tag-name>
		<source>tags/textArea.xhtml</source>
		<attribute>
			<name>value</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>rows</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>row</tag-name>
		<source>tags/row.xhtml</source>
		<attribute>
			<name>line</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>rowWithLabel</tag-name>
		<source>tags/rowWithLabel.xhtml</source>
		<attribute>
			<name>label</name>
		</attribute>
		<attribute>
			<name>line</name>
		</attribute>
		<attribute>
			<name>fCol</name>
		</attribute>
		<attribute>
			<name>sCol</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>dropdown</tag-name>
		<source>tags/dropdown.xhtml</source>
		<attribute>
			<name>value</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>list</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>label</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>datepicker</tag-name>
		<source>tags/datepicker.xhtml</source>
		<attribute>
			<name>value</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>mode</name>
			<!-- popup modes: inline, popup, plain -->
		</attribute>
		<attribute>
			<name>autoPopup</name>
		</attribute>
		<attribute>
			<name>showIcon</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>pageHeader</tag-name>
		<source>tags/pageHeader.xhtml</source>
		<attribute>
			<name>title</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>editBean</name>
		</attribute>
		<attribute>
			<name>editMethod</name>
		</attribute>
		<attribute>
			<name>saveBean</name>
		</attribute>
		<attribute>
			<name>saveMethod</name>
		</attribute>
		<attribute>
			<name>undoBean</name>
		</attribute>
		<attribute>
			<name>undoMethod</name>
		</attribute>
		<attribute>
			<name>saveButton</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>modalFooter</tag-name>
		<source>tags/modalFooter.xhtml</source>
		<attribute>
			<name>mode</name>
			<required>true</required>
			<!-- create, modify, select, save -->
		</attribute>
		<attribute>
			<name>action</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>deleteAction</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>table</tag-name>
		<source>tags/table.xhtml</source>
	</tag>
	<tag>
		<tag-name>entityLink</tag-name>
		<source>tags/entityLink.xhtml</source>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>mode</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>entity</name>
			<required>true</required>
		</attribute>
	</tag>

	<tag>
		<tag-name>addressModal</tag-name>
		<source>tags/modals/addressModal.xhtml</source>
		<attribute>
			<name>scout</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>styleClass</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>parentModal</tag-name>
		<source>tags/modals/parentModal.xhtml</source>
		<attribute>
			<name>scout</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>styleClass</name>
		</attribute>
		<attribute>
			<name>parent</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>promiseModal</tag-name>
		<source>tags/modals/promiseModal.xhtml</source>
		<attribute>
			<name>edit</name>
		</attribute>
		<attribute>
			<name>styleClass</name>
		</attribute>
		<attribute>
			<name>action</name>
		</attribute>
		<attribute>
			<name>deleteAction</name>
		</attribute>
	</tag>

</facelet-taglib>